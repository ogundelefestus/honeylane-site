<style type="text/css">
	*{
	    font-family: 'Ubuntu', sans-serif;
	}
</style>
<div class="container bg-info p-5">
  <div style="padding: 15px; background-color: white;">
    <a href="/admin/dashboard"> Admin Dashboard</a>
  </div>
</div>
<div class="container" style="margin-top: 10px;">
	<h2 style="text-align: center;">Students Results Page - Manage Students Result Here</h2>
	<table class="table table-hover">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Student Name</th>
                <th scope="col">Class</th>
                <th scope="col">Age</th>
                <th scope="col">Manage Result </th>
              </tr>
            </thead>
            {{# each students}}
            <tbody>
              
              <tr>
                <th scope="row">1</th>
                <td>{{ fullname }}</td>
                <td>{{ class }}</td>
                <td>{{ age }} years old</td>
                <td>
                    <!-- <a class="btn btn-info" role="button" data-toggle="modal" data-target="#editModal">Edit</a> -->
                    <a class="btn btn-sm btn-danger" href="#"><i class="fas fa-trash-alt"></i> manage</a>    
                </td>
              </tr>
            </tbody>
            {{/each}}
          </table>
</div>